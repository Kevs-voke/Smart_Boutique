            <HBox layoutX="10.0" layoutY="10.0" prefHeight="37.0" prefWidth="963.0" styleClass="enterText">
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Confirm  Password"
                      wrappingWidth="186.62667083740234"/>
                <TextField fx:id="confPass_txt"/>
            </HBox>
        </VBox>
        <Button fx:id="pass_btn" mnemonicParsing="false" prefHeight="25.0" prefWidth="132.0" text="OK"/>
    </VBox>
    <HBox layoutX="350.0" layoutY="30.0" prefHeight="45.0" prefWidth="349.0" styleClass="header">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Manage Your Profile"/>
