        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Change Your Email" wrappingWidth="164.55584716796875"/>
        <HBox prefHeight="94.0" prefWidth="963.0" styleClass="enterText">
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Your New Email"
                  wrappingWidth="164.6410140991211"/>
            <TextField fx:id="email_txt" prefHeight="26.0" prefWidth="336.0"/>
        </HBox>
        <Button fx:id="email_btn" mnemonicParsing="false" prefHeight="25.0" prefWidth="132.0" text="OK"/>
    </VBox>
    <VBox alignment="CENTER" layoutX="30.0" layoutY="184.0" prefHeight="215.0" prefWidth="963.0"
          styleClass="entries_container">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Change your Password"/>
        <Label fx:id="chPassword_lbl" prefHeight="20.0" prefWidth="171.0"/>
